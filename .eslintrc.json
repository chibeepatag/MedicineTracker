{
    "extends": "airbnb",
    "parserOptions": {
       "sourceType": "module",
       "ecmaFeatures": {
           "jsx": true
       }
   },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
      "semi": [2, "never"],
      "no-unexpected-multiline": 2,
      "react/no-array-index-key": "warn"
    }
}
